{
  "name": "search-user-list",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://Scherlock90.github.io/search-user-list/",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "set ENV=test && jest --runInBand",
    "test:snapshot-updated": "jest -u",
    "coverage": "jest --coverage",
    "eject": "react-app-rewired eject",
    "prettier:watch": "onchange \"**/*.+(js|jsx|ts|tsx|json|css|md)\" -- prettier --write {{changed}}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "postinstall": "node-prune"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.1",
    "typescript": "^4.6.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/lodash.flow": "^3.5.6",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.24.0",
    "axios-mock-adapter": "^1.20.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.4",
    "node-prune": "^1.0.2",
    "onchange": "^7.1.0",
    "prettier": "^2.3.0",
    "react-app-rewire-alias": "^1.0.1",
    "react-app-rewired": "^2.1.8",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1"
  }
}
